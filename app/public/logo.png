          setSelectedSubject(subject);
          setCurrentStep('levels');
        }} /&gt;
      )}

      {currentStep === 'levels' &amp;&amp; (
        &lt;LevelSelection 
          subject={selectedSubject} 
          onSelect={(level) =&gt; {
            setSelectedLevel(level);
            setCurrentStep('quiz');
          }} 
