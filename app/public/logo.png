zed="" mat-ripple-loader-class-name="mat-mdc-button-ripple" mat-ripple-loader-centered="" jslog="179062;track:generic_click,impression;BardVeMetadataKey:[[&quot;r_f79e2586f0b17592&quot;,&quot;c_075a4c07e379ac71&quot;,null,&quot;rc_5fd591ee77294e69&quot;,null,null,&quot;en&quot;,null,1,null,null,1,0]];mutable:true"><span class="mat-mdc-button-persistent-ripple mdc-icon-button__ripple"></span><mat-icon _ngcontent-ng-c341887062="" role="img" fonticon="content_copy" class="mat-icon notranslate gds-icon-s google-symbols mat-ligature-font mat-icon-no-color" aria-hidden="true" data-mat-icon-type="font" data-mat-icon-name="content_copy"></mat-icon><span class="mat-focus-indicator"></span><span class="mat-mdc-button-touch-target"></span></button><!----><!----></div><!----></div><!----><div _ngcontent-ng-c341887062="" class="formatted-code-block-internal-container ng-tns-c341887062-713"><div _ngcontent-ng-c341887062="" class="animated-opacity ng-tns-c341887062-713"><pre _ngcontent-ng-c341887062="" class="ng-tns-c341887062-713"><code _ngcontent-ng-c341887062="" role="text" data-test-id="code-content" class="code-container formatted ng-tns-c341887062-713">import React, { useState, useEffect } from 'react';
import { useNavigate, useParams } from 'react-router-dom';
import { motion, AnimatePresence } from 'framer-motion';
import { useGameStore } from '../../store/gameStore';
import { QUIZ_DATABASE, SUBJECTS_CONFIG } from '../../data/quizData';
import { Check, X, ArrowRight, Zap } from 'lucide-react';
import confetti from 'canvas-confetti';

export default function QuizEngine() {
  const navigate = useNavigate();
  const { subjectId } = useParams();
  const { addPoints, score, streak } = useGameStore();
  
  const [currentIdx, setCurrentIdx] = useState(0);
  const [selected, setSelected] = useState&lt;number | null&gt;(null);
  const [isFinished, setIsFinished] = useState(false);

  const questions = QUIZ_DATABASE.math.easy.slice(0, 10);
  const currentQ = questions[currentIdx];

  const handleAnswer = (idx: number) =&gt; {
    if (selected !== null) return;
    setSelected(idx);
    const isCorrect = idx === currentQ.correctAnswerIndex;
    
    addPoints(isCorrect);
    
    if (isCorrect) {
      if (streak &gt;= 2) confetti({ particleCount: 100, spread: 70, origin: { y: 0.6 } });
    }
  };

  const nextQuestion = () =&gt; {
    if (currentIdx &lt; questions.length - 1) {
      setCurrentIdx(currentIdx + 1);
      setSelected(null);
    } else {
      setIsFinished(true);
      confetti({ particleCount: 200, spread: 100 });
    }
  };

  if (isFinished) return (
    &lt;div className="flex flex-col items-center justify-center h-screen p-8 text-center bg-[#121212]"&gt;
      &lt;motion.div initial={{ scale: 0 }} animate={{ scale: 1 }} className="text-6xl mb-4"&gt;üèÜ&lt;/motion.div&gt;
      &lt;h2 className="text-4xl font-black text-white mb-2"&gt;TERMIN√â !&lt;/h2&gt;
      &lt;p className="text-gray-400 mb-8 text-xl"&gt;Ton score : &lt;span className="text-blue-500 font-bold"&gt;{score}&lt;/span&gt;&lt;/p&gt;
      &lt;button 
        onClick={() =&gt; navigate('/games')}
        className="w-full bg-blue-600 hover:bg-blue-500 text-white font-black py-4 rounded-3xl transition-all shadow-xl shadow-blue-600/20"
      &gt;
        RETOUR AU MENU
      &lt;/button&gt;
    &lt;/div&gt;
  );

  return (
    &lt;div className="min-h-screen bg-[#121212] p-6 flex flex-col"&gt;
      {/* Header Info */}
      &lt;div className="flex justify-between items-center mb-6"&gt;
        &lt;div className="bg-white/5 px-4 py-2 rounded-2xl flex items-center gap-2"&gt;
          &lt;Zap size={18} className="text-yellow-500 fill-yellow-500" /&gt;
          &lt;span className="font-bold"&gt;{streak}&lt;/span&gt;
        &lt;/div&gt;
        &lt;div className="text-xl font-black text-blue-500"&gt;{score} pts&lt;/div&gt;
      &lt;/div&gt;

      {/* Progress Bar */}
      &lt;div className="h-2 w-full bg-white/5 rounded-full mb-10 overflow-hidden"&gt;
        &lt;motion.div 
          className="h-full bg-blue-500"
          initial={{ width: 0 }}
          animate={{ width: `${((currentIdx + 1) / questions.length) * 100}%` }}
        /&gt;
      &lt;/div&gt;

      &lt;AnimatePresence mode="wait"&gt;
        &lt;motion.div
          key={currentIdx}
          initial={{ x: 50, opacity: 0 }} animate={{ x: 0, opacity: 1 }} exit={{ x: -50, opacity: 0 }}
          className="flex-1"
        &gt;
          &lt;h2 className="text-2xl font-bold text-white mb-8 leading-tight"&gt;
            {currentQ.text}
          &lt;/h2&gt;

          &lt;div className="grid gap-4"&gt;
            {currentQ.options.map((option: string, i: number) =&gt; {
              const isCorrect = i === currentQ.correctAnswerIndex;
              const isSelected = selected === i;
              
              let bgColor = "bg-white/5 border-transparent";
              if (selected !== null) {
                if (isCorrect) bgColor = "bg-green-500/20 border-green-500 text-green-500";
                else if (isSelected) bgColor = "bg-red-500/20 border-red-500 text-red-500";
                else bgColor = "bg-white/5 opacity-30 border-transparent";
              }

              return (
                &lt;button
                  key={i}
                  disabled={selected !== null}
                  onClick={() =&gt; handleAnswer(i)}
                  className={`w-full p-5 rounded-3xl text-left font-bold text-lg border-2 transition-all flex justify-between items-center ${bgColor}`}
                &gt;
                  {option}
                  {selected !== null &amp;&amp; isCorrect &amp;&amp; &lt;Check size={24} /&gt;}
                  {selected !== null &amp;&amp; isSelected &amp;&amp; !isCorrect &amp;&amp; &lt;X size={24} /&gt;}
                &lt;/button&gt;
              );
            })}
          &lt;/div&gt;
        &lt;/motion.div&gt;
      &lt;/AnimatePresence&gt;

      {selected !== null &amp;&amp; (
        &lt;motion.button
          initial={{ y: 20, opacity: 0 }} animate={{ y: 0, opacity: 1 }}
          onClick={nextQuestion}
          className="bg-white text-black font-black py-4 rounded-3xl flex items-center justify-center gap-2 shadow-xl"
        &gt;
          SUIVANT &lt;ArrowRight size={20} /&gt;
        &lt;/motion.button&gt;
      )}
    &lt;/div&gt;
  );
