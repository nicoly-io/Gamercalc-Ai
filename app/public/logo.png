    if (feedback !== null || isAnimating) return; // Bloquer si d√©j√† r√©pondu

    setSelectedOption(index);
    setIsAnimating(true);

    const result = store.submitAnswer(index);
    setFeedback({ isCorrect: result.correct, points: result.pointsEarned });

    // Feedback Sonore
    if (result.correct) {
       // playCorrect();
       if (streak &gt; 2) confetti({ particleCount: 50, spread: 50, origin: { y: 0.8 } });
    } else {
       // playWrong();
    }

    // Attendre un peu avant de passer √† la suite
    setTimeout(() =&gt; {
        setIsAnimating(false);
    }, 2000);
  };

  const handleNext = () =&gt; {
      setSelectedOption(null);
      setFeedback(null);
      store.nextQuestion();
  }

  // --- √âCRAN DE FIN DE PARTIE ---
  if (gameStatus === 'finished') {
    return (
        &lt;div className="flex flex-col items-center justify-center h-full p-6 text-center"&gt;
            &lt;motion.div 
                initial={{ scale: 0 }} animate={{ scale: 1, rotate: 360 }} transition={{type:"spring"}}
                className="bg-yellow-500/20 p-6 rounded-full mb-6"
            &gt;
                &lt;Trophy className="w-24 h-24 text-yellow-500" /&gt;
            &lt;/motion.div&gt;
            &lt;h2 className="text-4xl font-bold text-white mb-2"&gt;Quiz Termin√© !&lt;/h2&gt;
            &lt;p className="text-xl text-gray-300 mb-8"&gt;Score final : &lt;span className="text-primary-color font-bold"&gt;{score} pts&lt;/span&gt;&lt;/p&gt;
            
            &lt;div className="flex gap-4 w-full max-w-sm"&gt;
                &lt;button onClick={() =&gt; { store.resetGame(); navigate('/games'); }} className="flex-1 bg-surface-light py-4 rounded-xl font-bold text-white"&gt;
                    Menu
                &lt;/button&gt;
                &lt;button onClick={() =&gt; { store.resetGame(); navigate(-1); }} className={`flex-1 bg-gradient-to-r ${subjectTheme.gradient} py-4 rounded-xl font-bold text-white`}&gt;
                    Rejouer
                &lt;/button&gt;
            &lt;/div&gt;
        &lt;/div&gt;
    );
  }

  // --- √âCRAN DU QUIZ EN COURS ---
  return (
    &lt;div className="flex flex-col h-full p-4 relative overflow-hidden"&gt;
      {/* Header : Barre de progression et Score */}
      &lt;div className="mb-6 pt-4"&gt;
        &lt;div className="flex justify-between items-center mb-3"&gt;
            &lt;button onClick={() =&gt; { store.resetGame(); navigate('/games'); }}&gt;&lt;X className="text-gray-400" /&gt;&lt;/button&gt;
            &lt;div className="flex flex-col items-end"&gt;
                &lt;span className="text-2xl font-bold text-primary-color"&gt;{score} pts&lt;/span&gt;
                {streak &gt; 1 &amp;&amp; &lt;span className="text-xs text-orange-400 animate-pulse"&gt;S√©rie : {streak} üî•&lt;/span&gt;}
            &lt;/div&gt;
        &lt;/div&gt;
        &lt;div className="h-3 w-full bg-surface-light rounded-full overflow-hidden relative"&gt;
            &lt;motion.div 
                initial={{ width: 0 }}
                animate={{ width: `${progressPercentage}%` }}
                className={`h-full bg-gradient-to-r ${subjectTheme.gradient}`}
            /&gt;
             &lt;div className="absolute top-0 left-0 w-full h-full flex items-center justify-center text-xs font-bold mix-blend-difference text-white"&gt;
                Question {currentQuestionIndex + 1} / {currentQuestions.length}
             &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;

      {/* Question */}
      &lt;AnimatePresence mode='wait'&gt;
        &lt;motion.div
            key={currentQuestion.id}
            initial={{ opacity: 0, x: 50 }}
            animate={{ opacity: 1, x: 0 }}
            exit={{ opacity: 0, x: -50 }}
            className="flex-1 flex flex-col justify-center"
        &gt;
            &lt;h2 className="text-2xl font-bold text-white mb-8 leading-relaxed"&gt;{currentQuestion.text}&lt;/h2&gt;

            {/* Options de r√©ponse */}
            &lt;div className={`flex flex-col gap-4 ${feedback &amp;&amp; !feedback.isCorrect ? 'animate-shake' : ''}`}&gt;
            {currentQuestion.options.map((option, index) =&gt; {
                // Logique de couleur complexe pour le feedback
                let btnClass = "bg-surface-light hover:bg-surface-light/80 border-2 border-transparent text-white";
                if (selectedOption !== null) {
                    if (index === currentQuestion.correctAnswerIndex) {
                        // C'est la bonne r√©ponse : vert
                        btnClass = "bg-green-600 text-white border-green-400";
                    } else if (index === selectedOption &amp;&amp; index !== currentQuestion.correctAnswerIndex) {
                        // C'est la mauvaise r√©ponse cliqu√©e : rouge
                        btnClass = "bg-red-600 text-white border-red-400";
                    } else {
                        // Les autres options inactives
                        btnClass = "bg-surface opacity-50 text-gray-400";
                    }
                }

                return (
                &lt;motion.button
                    key={index}
                    whileTap={feedback === null ? { scale: 0.97 } : {}}
                    onClick={() =&gt; handleOptionClick(index)}
                    disabled={feedback !== null}
                    className={`w-full p-5 rounded-xl text-left font-semibold text-lg transition-all relative flex items-center justify-between ${btnClass}`}
                &gt;
                    &lt;span&gt;{option}&lt;/span&gt;
                    {/* Ic√¥nes de feedback sur le bouton */}
                    {selectedOption !== null &amp;&amp; index === currentQuestion.correctAnswerIndex &amp;&amp; &lt;CheckCircle2 className="w-6 h-6 text-white" /&gt;}
                    {selectedOption === index &amp;&amp; index !== currentQuestion.correctAnswerIndex &amp;&amp; &lt;XCircle className="w-6 h-6 text-white" /&gt;}
                &lt;/motion.button&gt;
                )
            })}
            &lt;/div&gt;
        &lt;/motion.div&gt;
      &lt;/AnimatePresence&gt;

      {/* Feedback Overlay et Bouton Suivant */}
      &lt;AnimatePresence&gt;
        {feedback &amp;&amp; (
          &lt;motion.div 
            initial={{ y: 100, opacity: 0 }}
            animate={{ y: 0, opacity: 1 }}
            exit={{ y: 100, opacity: 0 }}
            className={`absolute bottom-0 left-0 right-0 p-6 rounded-t-3xl flex items-center justify-between shadow-2xl backdrop-blur-xl
                        ${feedback.isCorrect ? 'bg-green-900/80 border-t-2 border-green-500' : 'bg-red-900/80 border-t-2 border-red-500'}`}
          &gt;
            &lt;div&gt;
                &lt;h3 className={`text-xl font-bold ${feedback.isCorrect ? 'text-green-300' : 'text-red-300'}`}&gt;
                    {feedback.isCorrect ? 'Excellente r√©ponse !' : 'Oups, pas tout √† fait.'}
                &lt;/h3&gt;
                {feedback.isCorrect &amp;&amp; &lt;p className="text-green-200"&gt;+{feedback.points} points&lt;/p&gt;}
                {!feedback.isCorrect &amp;&amp; currentQuestion.explanation &amp;&amp; (
                    &lt;p className="text-sm text-red-200 mt-1"&gt;üí° {currentQuestion.explanation}&lt;/p&gt;
                )}
            &lt;/div&gt;
            &lt;button 
                onClick={handleNext}
                className={`px-6 py-3 rounded-full font-bold flex items-center gap-2 transition-transform active:scale-95
                             ${feedback.isCorrect ? 'bg-green-500 text-black hover:bg-green-400' : 'bg-red-500 text-white hover:bg-red-400'}`}
            &gt;
                Suivant &lt;ArrowRight className="w-5 h-5" /&gt;
            &lt;/button&gt;
          &lt;/motion.div&gt;
        )}
      &lt;/AnimatePresence&gt;
    &lt;/div&gt;
  );
};

