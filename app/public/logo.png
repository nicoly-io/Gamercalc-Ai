<span class="hljs-keyword">const</span> QUESTIONS_PER_GAME = <span class="hljs-number">10</span>; <span class="hljs-comment">// Nombre de questions par session</span>

<span class="hljs-keyword">export</span> <span class="hljs-keyword">const</span> useGameStore = create&lt;GameState&gt;(<span class="hljs-function">(<span class="hljs-params">set, get</span>) =&gt;</span> ({
  <span class="hljs-attr">isPaidUser</span>: <span class="hljs-literal">false</span>, <span class="hljs-comment">// Mettre √† true pour tester sans payer</span>
  <span class="hljs-attr">selectedSubject</span>: <span class="hljs-literal">null</span>,
  <span class="hljs-attr">selectedDifficulty</span>: <span class="hljs-literal">null</span>,
  <span class="hljs-attr">currentQuestions</span>: [],
  <span class="hljs-attr">currentQuestionIndex</span>: <span class="hljs-number">0</span>,
  <span class="hljs-attr">score</span>: <span class="hljs-number">0</span>,
  <span class="hljs-attr">streak</span>: <span class="hljs-number">0</span>,
  <span class="hljs-attr">gameStatus</span>: <span class="hljs-string">'idle'</span>,

  <span class="hljs-attr">unlockPremium</span>: <span class="hljs-function">() =&gt;</span> set({ <span class="hljs-attr">isPaidUser</span>: <span class="hljs-literal">true</span> }),

  <span class="hljs-attr">startGame</span>: <span class="hljs-function">(<span class="hljs-params">subject, difficulty</span>) =&gt;</span> {
    <span class="hljs-comment">// S√©lectionner al√©atoirement 10 questions dans la base</span>
    <span class="hljs-keyword">const</span> allQuestions = QUIZ_DATABASE[subject][difficulty];
    <span class="hljs-keyword">const</span> shuffled = [...allQuestions].sort(<span class="hljs-function">() =&gt;</span> <span class="hljs-number">0.5</span> - <span class="hljs-built_in">Math</span>.random());
    <span class="hljs-keyword">const</span> selected = shuffled.slice(<span class="hljs-number">0</span>, QUESTIONS_PER_GAME);

    set({
      <span class="hljs-attr">selectedSubject</span>: subject,
      <span class="hljs-attr">selectedDifficulty</span>: difficulty,
      <span class="hljs-attr">currentQuestions</span>: selected,
      <span class="hljs-attr">currentQuestionIndex</span>: <span class="hljs-number">0</span>,
      <span class="hljs-attr">score</span>: <span class="hljs-number">0</span>,
      <span class="hljs-attr">streak</span>: <span class="hljs-number">0</span>,
      <span class="hljs-attr">gameStatus</span>: <span class="hljs-string">'playing'</span>
    });
  },

  <span class="hljs-attr">submitAnswer</span>: <span class="hljs-function">(<span class="hljs-params">answerIndex</span>) =&gt;</span> {
    <span class="hljs-keyword">const</span> { currentQuestions, currentQuestionIndex, streak, score } = get();
    <span class="hljs-keyword">const</span> currentQ = currentQuestions[currentQuestionIndex];
    <span class="hljs-keyword">const</span> isCorrect = answerIndex === currentQ.correctAnswerIndex;

    <span class="hljs-keyword">let</span> pointsEarned = <span class="hljs-number">0</span>;
    <span class="hljs-keyword">let</span> newStreak = streak;
    <span class="hljs-keyword">let</span> newScore = score;

    <span class="hljs-keyword">if</span> (isCorrect) {
      newStreak = streak + <span class="hljs-number">1</span>;
      <span class="hljs-comment">// Syst√®me de points dynamique : Bonus de s√©rie</span>
      <span class="hljs-keyword">const</span> streakBonus = newStreak &gt;= <span class="hljs-number">3</span> ? <span class="hljs-built_in">Math</span>.min(newStreak * <span class="hljs-number">10</span>, <span class="hljs-number">50</span>) : <span class="hljs-number">0</span>;
      pointsEarned = <span class="hljs-number">100</span> + streakBonus;
      newScore += pointsEarned;
    } <span class="hljs-keyword">else</span> {
      newStreak = <span class="hljs-number">0</span>;
      <span class="hljs-comment">// Malus l√©ger</span>
      newScore = <span class="hljs-built_in">Math</span>.max(<span class="hljs-number">0</span>, score - <span class="hljs-number">20</span>);
    }

    set({ <span class="hljs-attr">score</span>: newScore, <span class="hljs-attr">streak</span>: newStreak });
    <span class="hljs-keyword">return</span> { <span class="hljs-attr">correct</span>: isCorrect, pointsEarned };
  },

  <span class="hljs-attr">nextQuestion</span>: <span class="hljs-function">() =&gt;</span> {
    <span class="hljs-keyword">const</span> { currentQuestionIndex, currentQuestions } = get();
    <span class="hljs-keyword">if</span> (currentQuestionIndex &lt; currentQuestions.length - <span class="hljs-number">1</span>) {
      set({ <span class="hljs-attr">currentQuestionIndex</span>: currentQuestionIndex + <span class="hljs-number">1</span> });
    } <span class="hljs-keyword">else</span> {
      set({ <span class="hljs-attr">gameStatus</span>: <span class="hljs-string">'finished'</span> });
    }
  },

