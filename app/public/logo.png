import { useParams, useNavigate } from 'react-router-dom';
import { useGameStore } from '../../store/gameStore';
import { QUIZ_DATABASE } from '../../data/quizData';
import confetti from 'canvas-confetti';

export default function QuizEngine() {
  const { id } = useParams();
  const nav = useNavigate();
  const { isPaid, addPoints } = useGameStore();
  const [idx, setIdx] = useState(0);

  // S√©curit√© : Si l'utilisateur force l'URL sans payer, on le renvoie
  useEffect(() =&gt; {
    if (!isPaid) nav('/games');
  }, [isPaid, nav]);

  const questions = QUIZ_DATABASE[id as keyof typeof QUIZ_DATABASE]?.easy || [];
  const q = questions[idx];

  if (!q) return null;

  const answer = (i: number) =&gt; {
    const ok = i === q.correct;
    addPoints(ok);
    if(ok) {
        confetti({ particleCount: 150, spread: 70, origin: { y: 0.6 } });
    }
    
    if(idx &lt; questions.length - 1) {
        setTimeout(() =&gt; setIdx(idx + 1), 500);
    } else {
        alert("F√©licitations ! Vous avez termin√© ce niveau.");
        nav('/games');
    }
  };

  return (
    &lt;div className="p-6 bg-[#121212] min-h-screen flex flex-col"&gt;
      &lt;div className="w-full bg-white/5 h-2 rounded-full mb-12"&gt;
        &lt;div className="bg-blue-500 h-full rounded-full transition-all duration-500" style={{width: `${((idx + 1)/questions.length)*100}%`}}&gt;&lt;/div&gt;
      &lt;/div&gt;

      &lt;div className="flex-1"&gt;
        &lt;span className="text-blue-500 font-black text-sm uppercase tracking-widest"&gt;Question {idx + 1}/{questions.length}&lt;/span&gt;
        &lt;h2 className="text-2xl font-bold text-white mt-4 mb-12 leading-tight"&gt;{q.text}&lt;/h2&gt;
        
        &lt;div className="grid gap-4"&gt;
            {q.options.map((o: string, i: number) =&gt; (
            &lt;button 
                key={i} 
                onClick={() =&gt; answer(i)} 
                className="p-6 bg-white/5 border border-white/10 rounded-[1.5rem] text-left font-bold text-white hover:bg-white/10 active:bg-blue-600 transition-all"
            &gt;
                {o}
            &lt;/button&gt;
            ))}
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
  );
}
