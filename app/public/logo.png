        &lt;/header&gt;
        
        &lt;main className="flex-1 overflow-y-auto page-transition"&gt;
          {renderContent()}
        &lt;/main&gt;
      &lt;/div&gt;
    &lt;/div&gt;
  );
};

export default App;
