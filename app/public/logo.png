import { motion } from 'framer-motion';
import { Delete, Equal, RotateCcw } from 'lucide-react';

export default function CalculatorScreen() {
  const [expression, setExpression] = useState('');
  const [result, setResult] = useState('');

  const btn = (val: string) =&gt; {
    setExpression(prev =&gt; prev + val);
  };

  const calculate = () =&gt; {
    try {
      // Pour la version simple, on utilise eval, mais le serveur Python prendra le relais pour le calcul formel
      const evalRes = eval(expression.replace('√ó', '*').replace('√∑', '/'));
      setResult(evalRes.toString());
    } catch (e) {
      setResult('Erreur');
    }
  };

  const clear = () =&gt; { setExpression(''); setResult(''); };

  return (
    &lt;div className="flex flex-col h-screen p-6 bg-[#121212]"&gt;
      &lt;div className="flex-1 flex flex-col justify-end items-end mb-8"&gt;
        &lt;div className="text-gray-400 text-2xl overflow-x-auto whitespace-nowrap w-full text-right mb-2"&gt;
          {expression || '0'}
        &lt;/div&gt;
        &lt;div className="text-white text-6xl font-bold"&gt;{result || '0'}&lt;/div&gt;
      &lt;/div&gt;

      &lt;div className="grid grid-cols-4 gap-3"&gt;
        {/* Ligne Fonctions */}
        &lt;button onClick={clear} className="h-16 rounded-2xl bg-red-500/20 text-red-500 font-bold"&gt;&lt;RotateCcw /&gt;&lt;/button&gt;
        &lt;button onClick={() =&gt; btn('(')} className="h-16 rounded-2xl bg-white/10 text-white font-bold"&gt;(&lt;/button&gt;
        &lt;button onClick={() =&gt; btn(')')} className="h-16 rounded-2xl bg-white/10 text-white font-bold"&gt;)&lt;/button&gt;
        &lt;button onClick={() =&gt; btn('/')} className="h-16 rounded-2xl bg-blue-500 text-white font-bold"&gt;√∑&lt;/button&gt;

        {/* Chiffres */}
